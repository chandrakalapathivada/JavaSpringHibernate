<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="employee" class="com.javatpoint.SpringExample1.Employee">
		<constructor-arg value="10"></constructor-arg>
		 <constructor-arg value="sono"></constructor-arg>
		  <constructor-arg><ref bean="adress"></ref></constructor-arg>
	</bean>
	<bean id="adress" class="com.javatpoint.SpringExample1.Address">
		<constructor-arg value="ap00"></constructor-arg>
		<constructor-arg value="apii"></constructor-arg>
		<constructor-arg value="ap"></constructor-arg>
	</bean>
</beans>
