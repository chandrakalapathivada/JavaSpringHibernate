package com.java.Interfaceeg;

interface abc
{
	void show();
}
class xyz implements abc
{

	@Override
	public void show() {
		// TODO Auto-generated method stub
		System.out.println("running................");
		
	}
	
}
public class Telusko {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		abc a=new xyz();
		a.show();

	}

}
